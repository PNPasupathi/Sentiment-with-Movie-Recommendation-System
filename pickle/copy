movie1=movstory(name[0])
movie2=movstory(name[1])
movie3=movstory(name[2])
movie4=movstory(name[3])
movie5=movstory(name[4])

def movie1res():
  movie1pos=analysepos(movie1)
  movie1neg=analyseneg(movie1)
  movie1neu=analyseneu(movie1)
  if movie1neu ==1:
      print('neutral')
  elif movie1neg ==1:
      print('negative')
  elif movie1pos==1:
      print('positive')

